package com.supinfo.suptravel.bean;

import java.util.Collection;

import javax.persistence.*;

@Entity
@Table(name="trip")
public class Trip {
	@Id
	@GeneratedValue
	private int id;
	
	@Basic(optional = false)
	@Column(name = "tripname", length = 200, nullable = false, unique = true)
    private String tripname;

	public String getTripname() {
		return tripname;
	}
	public void setTripname(String tripname) {
		this.tripname = tripname;
	}
	
	@Column(name = "campus", length = 200, nullable = false, unique = true)
    private String campus;

	public String getCampus() {
		return campus;
	}
	public void setCampus(String campus) {
		this.campus = campus;
	}
	
	@JoinColumn(name="tripbag", referencedColumnName="trip", updatable = false)
	@OneToMany(fetch = FetchType.LAZY, cascade = CascadeType.ALL, mappedBy = "trip")
	private Collection<Tripbag> TribagCollection;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}

}
